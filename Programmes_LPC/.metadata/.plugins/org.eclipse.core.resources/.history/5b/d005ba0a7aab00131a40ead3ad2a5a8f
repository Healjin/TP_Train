#ifdef __USE_CMSIS
#include "LPC17xx.h"
#include "Ecran.h"
#endif


int main(void) {

	Init_display();
	Set_cursor(0,0);
	/* Taille Ã©cran = 320x240 */
	Create_partial_screen(0, 319, 0, 239);
	while(1) {
		Write_pixel(0,0,255);
	}
	return 0 ;
}
